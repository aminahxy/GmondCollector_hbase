function toggleMoreFromIntuit(){$("#MoreFromIntuit").toggleClass("MoreFromIntuit-Open"),$("#MoreFromIntuitIcon").toggleClass("MoreFromIntuitIcon-Open")}$(document).ready(function(){Intuit.CreateModal("hiddensignIn","globalLoginbox",!0),Intuit.CreateModal("SignInButton","LoginboxRedirect",!0),$(".MoreFromIntuitLink").click(function(n){n.preventDefault(),toggleMoreFromIntuit()}),$("#closeMoreFromIntuit").click(function(n){n.preventDefault(),toggleMoreFromIntuit()}),$("#searchTerm").keypress(function(n){var r=n.keyCode?n.keyCode:n.which,t,i;r==13&&(t=$("#searchTerm").val(),i="http://www.intuit.com/sbweb/common/catalog/category/internal_search/search_redirect.jsp?searchTerm="+t,window.open(i,"_blank"))}),Intuit.BindSuggestedSearch("searchSupportSite","","","Search keywords or errors"),$("#searchSupportSite").ghostText()})